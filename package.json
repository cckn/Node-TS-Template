    "start": "ts-node src/index.ts"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "^10.0.3",
    "jest": "^25.5.4",
    "nodemon": "^2.0.2",
    "supertest": "^6.0.1",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.8.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.0",
    "typescript": "^3.9.7"
  },
  "author": "",
  "license": "ISC",
